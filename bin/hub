#!/usr/bin/env ruby

require File.expand_path(File.join(File.dirname(__FILE__), '..', 'lib', 'hubmaster.rb'))

if ARGV.length > 0
  result = case ARGV[0]
    when "repos"
      case ARGV[1]
        when "-view"
          Github.connect
          Github::Repos.view
        else
          puts "Unknown"
        end
    else
      puts "Unknown"
    end
    exit
else
  puts "Instructions Here"
end

